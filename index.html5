<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Default Form Values</title>
</head>
<body>

<h2>Form With Default Values</h2>

<form>

    <!-- Text input -->
    <label>Name:</label>
    <input type="text" value="John Doe"><br><br>

    <!-- Email input -->
    <label>Email:</label>
    <input type="email" value="john@example.com"><br><br>

    <!-- Number input -->
    <label>Age:</label>
    <input type="number" value="30"><br><br>

    <!-- Password input -->
    <label>Password:</label>
    <input type="password" value="123456"><br><br>

    <!-- Date input -->
    <label>Date of Birth:</label>
    <input type="date" value="1995-10-15"><br><br>

    <!-- Radio buttons -->
    <label>Gender:</label><br>
    <input type="radio" name="gender" checked> Male<br>
    <input type="radio" name="gender"> Female<br><br>

    <!-- Checkbox -->
    <label>Skills:</label><br>
    <input type="checkbox" checked> HTML<br>
    <input type="checkbox" checked> CSS<br>
    <input type="checkbox"> JavaScript<br><br>

    <!-- Select dropdown -->
    <label>Country:</label>
    <select>
        <option>India</option>
        <option selected>USA</option>
        <option>UK</option>
    </select><br><br>

    <!-- Textarea -->
    <label>Address:</label><br>
    <textarea rows="3">123 Default Street, New York</textarea><br><br>

    <!-- Button -->
    <button>Submit</button>

</form>

</body>
</html>
